{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "linebreak-style": 0,
        "no-useless-constructor": "off",
        "no-unused-vars": "off",
        "no-empty-function": "off",
        "no-unused-expressions": "off",
        "object-curly-newline": "off",
        "class-methods-use-this": "off"
    }
}
